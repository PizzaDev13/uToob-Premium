{
  "manifest_version": 3,
  "name": "uToob - Premium | Best Youtube AdBlocker",
  "version": "42",
  "permissions": ["activeTab"],
  "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "action": {
    "default_title": "",
    "default_popup": "index.html"
  },
  "content_scripts": [
      {
          "matches": ["*://www.youtube.com/*"],
          "js": ["content.js"]
      }
  ]
}